<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout10"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundwallet2"
    tools:context=".MainMenuFragment">

    <ImageButton
        android:id="@+id/ContactBtn"
        android:layout_width="136dp"
        android:layout_height="156dp"
        android:background="#00FFFFFF"
        android:contentDescription="TransactionHistory"
        android:scaleType="fitCenter"
        android:src="@drawable/contactusicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/TopupBtnMain"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.151" />

    <ImageView
        android:id="@+id/TitleTV"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_width="292dp"
        android:layout_height="151dp"
        android:contentDescription="Title"
        android:src="@drawable/title1"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="318dp" />

    <ImageButton
        android:id="@+id/TopupBtnMain"
        android:layout_width="136dp"
        android:layout_height="156dp"
        android:background="#00FFFFFF"
        android:contentDescription="TopUp"
        android:scaleType="fitCenter"
        android:src="@drawable/topupicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/BookBtn"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.152" />

    <ImageView
        android:id="@+id/imageView7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TitleTV"
        app:layout_constraintVertical_bias="0.039"
        android:layout_width="228dp"
        android:layout_height="78dp"
        android:src="@drawable/background2"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="612dp" />

    <TextView
        android:id="@+id/TVAmountMain"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.538"
        android:layout_width="146dp"
        android:layout_height="39dp"
        android:lineSpacingExtra="10sp"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="635dp" />

    <ImageView
        android:id="@+id/imageView8"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.538"
        android:layout_width="51dp"
        android:layout_height="39dp"
        android:src="@drawable/walleticon"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="537dp" />

    <TextView
        android:id="@+id/TVType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TopupBtn"
        app:layout_constraintVertical_bias="0.202" />

    <ImageButton
        android:id="@+id/DriverApplyBtn2"
        android:layout_width="351dp"
        android:layout_height="130dp"
        android:layout_marginBottom="92dp"
        android:scaleType="center"
        android:src="@drawable/driverpromo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TopupBtn"
        app:layout_constraintVertical_bias="0.319" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="PROMO"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/TVType"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/DriverApplyBtn2"
        app:layout_constraintTop_toBottomOf="@+id/BookBtn"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/BookBtn"
        android:layout_width="127dp"
        android:layout_height="158dp"
        android:src="@drawable/bookcarbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.017"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.153" />

</androidx.constraintlayout.widget.ConstraintLayout>